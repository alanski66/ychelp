{
  "require": {
    "aelvan/imager": "v2.4.0",
    "by/guest-entries-notification": "1.0.0",
    "craftcms/cms": "3.5.15.1",
    "craftcms/feed-me": "4.3.1",
    "craftcms/guest-entries": "2.3.0",
    "craftcms/redactor": "2.8.3",
    "dolphiq/iconpicker": "1.0.5",
    "hybridinteractive/craft-position-fieldtype": "1.0.16",
    "joomkit/pdfthumbnailer": "^1.0",
    "nystudio107/craft-seomatic": "3.3.22",
    "nystudio107/craft-typogrify": "1.1.18",
    "pennebaker/craft-architect": "2.3.3",
    "studioespresso/craft-splashingimages": "2.1.2",
    "superbig/craft3-mobiledetect": "1.0.2",
    "verbb/cloner": "1.2.1",
    "verbb/super-table": "2.6.3",
    "vlucas/phpdotenv": "^3.4.0",
    "xpertbot/craft-wheelform": "2.5.2",
    "xpertbot/wheelformhelper": "^1.1"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "repositories": [
    {
      "type": "path",
      "url": "plugins/joomkit/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "plugins/xpertbot/*",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
