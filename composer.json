{
  "require": {
    "aelvan/imager": "v2.4.0",
    "craftcms/cms": "^3.4.25",
    "craftcms/redactor": "2.7.1",
    "ether/seo": "3.6.6",
    "hybridinteractive/craft-position-fieldtype": "1.0.16",
    "pennebaker/craft-architect": "2.3.3",
    "superbig/craft3-mobiledetect": "1.0.2",
    "verbb/super-table": "2.6.1",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
